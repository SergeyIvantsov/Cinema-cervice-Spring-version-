<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
        https://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.29.xsd">


<changeSet id="create_dir" author="sergey">
        <createTable tableName="directors">
            <column name="id_director" type="bigint" autoIncrement="true">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="director_name" type="VARCHAR(255)">
                <constraints nullable="false"/>
            </column>
            <column name="director_surname" type="VARCHAR(255)">
                <constraints nullable="false"/>
            </column>

        </createTable>
    </changeSet>





</databaseChangeLog>
